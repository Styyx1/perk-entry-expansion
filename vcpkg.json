    "port-version": 0,
    "description": "An SKSE plugin that adds a perk entry point library for other mods to use.",
    "homepage": "https://www.skyrimng.com",
    "license": "Apache-2.0",
    "features": {
        "plugin": {
            "description": "Build the SKSE plugin.",
          "dependencies": [
            "commonlibsse-ng",
            "xbyak",
            "directxmath",
            "directxtk",
            "openvr",
            "magic-enum"
